		"@testing-library/user-event": "^14.5.2",
		"@types/lodash": "^4.17.7",
		"@types/node": "^20.14.11",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"@vitejs/plugin-react-swc": "^3.7.0",
		"@vitest/coverage-v8": "^1.6.0",
