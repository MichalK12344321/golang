{
	"name": "web",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"prettier": "prettier --write .",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"coverage": "vitest run --coverage",
		"cdg:client": "npx nswag run ./src/api/client/cdg/client.nswag.json",
		"scheduler:client": "npx nswag run ./src/api/client/scheduler/client.nswag.json",
		"build-dev": "tsc && vite build --outDir=./../../../dist --emptyOutDir"
	},
	"dependencies": {
		"axios": "1.7.2",
		"date-fns": "^3.6.0",
		"happy-dom": "^13.10.1",
		"localforage": "^1.10.0",
		"lodash": "^4.17.21",
		"match-sorter": "^6.3.4",
		"mitt": "^3.0.1",
		"moment": "^2.30.1",
		"msw": "^2.3.1",
		"nswag": "14.0.8",
		"primeflex": "^3.3.1",
		"primeicons": "^6.0.1",
		"primereact": "^10.7.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-hook-form": "^7.52.1",
		"react-idle-timer": "^5.7.2",
		"react-query": "^3.39.3",
		"react-router-dom": "^6.25.0",
		"react-use-websocket": "^4.8.1",
		"resolvers": "^1.0.0",
		"retry-axios": "^3.1.3",
		"sort-by": "^1.2.0",
		"tailwindcss": "^3.4.6",
		"zod": "^3.23.8"
